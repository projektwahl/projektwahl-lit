{
  "name": "projektwahl-lit",
  "version": "0.0.1",
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "Moritz Hedtke",
    "email": "Moritz.Hedtke@t-online.de"
  },
  "bugs": {
    "url": "https://github.com/mohe2015/projektwahl-lit/issues"
  },
  "homepage": "https://github.com/mohe2015/projektwahl-lit",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohe2015/projektwahl-lit.git"
  },
  "description": "Software to manage choosing projects and automatically assigning people to projects.",
  "type": "module",
  "scripts": {
    "format": "prettier --ignore-path .prettierignore --write .",
    "format:check": "prettier --ignore-path .prettierignore --check .",
    "test": "node --experimental-loader ./src/loader.js --enable-source-maps tests/e2e/new-tests.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "evaluate": "node --experimental-loader ./src/loader.js --enable-source-maps ./src/server/routes/evaluate/index.ts",
    "setup": "node --experimental-loader ./src/loader.js --enable-source-maps ./src/server/setup.ts",
    "server": "node --experimental-loader ./src/loader.js --enable-source-maps --experimental-import-meta-resolve ./src/server/index.ts",
    "localize-extract": "lit-localize extract",
    "localize-build": "lit-localize build",
    "typecheck": "tsc",
    "lit-analyze": "lit-analyzer --strict --rules.no-unknown-attribute off src",
    "build": "node build.js"
  },
  "dependencies": {
    "@dev.mohe/argon2": "^0.29.4",
    "@lit-labs/motion": "^1.0.2",
    "@lit-labs/task": "^1.1.1",
    "@lit/localize": "^0.11.2",
    "bootstrap": "^5.1.3",
    "cookie": "^0.4.2",
    "csv-parse": "^5.0.4",
    "js-cookie": "^3.0.1",
    "lit": "^2.2.1",
    "openid-client": "^5.1.4",
    "postgres": "^3.0.4",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "chance": "^1.1.8",
    "@types/cookie": "^0.4.1",
    "@types/js-cookie": "^3.0.1",
    "@types/node": "^17.0.23",
    "@types/selenium-webdriver": "^4.0.18",
    "selenium-webdriver": "^4.1.1",
    "typescript": "^4.6.3",
    "@types/chance": "^1.1.3",
    "esbuild": "^0.14.32",
    "esbuild-linux-64": "^0.14.32"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.0",
    "@lit/localize-tools": "^0.6.1",
    "purgecss": "^4.1.3"
  }
}
