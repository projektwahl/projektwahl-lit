{
  "name": "projektwahl-lit",
  "version": "0.0.1",
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "Moritz Hedtke",
    "email": "Moritz.Hedtke@t-online.de"
  },
  "bugs": {
    "url": "https://github.com/mohe2015/projektwahl-lit/issues"
  },
  "homepage": "https://github.com/mohe2015/projektwahl-lit",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohe2015/projektwahl-lit.git"
  },
  "description": "Software to manage choosing projects and automatically assigning people to projects.",
  "type": "module",
  "scripts": {
    "format": "prettier --ignore-path .gitignore --write .",
    "format:check": "prettier --ignore-path .gitignore --check .",
    "test": "SELENIUM_BROWSER=chrome node  --experimental-loader ./src/loader.js --enable-source-maps tests/e2e/welcome.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "evaluate": "node --experimental-loader ./src/loader.js --enable-source-maps ./src/server/routes/evaluate/index.js",
    "setup": "node --experimental-loader ./src/loader.js --enable-source-maps ./src/server/setup.js",
    "server": "node --experimental-loader ./src/loader.js --enable-source-maps --experimental-import-meta-resolve ./src/server/index.js",
    "localize-extract": "lit-localize extract",
    "typecheck": "tsc",
    "lit-analyze": "lit-analyzer --strict --rules.no-unknown-attribute off src",
    "build": "node build.js"
  },
  "dependencies": {
    "@dev.mohe/argon2": "^0.29.0",
    "@lit-labs/motion": "^1.0.2",
    "@lit-labs/task": "^1.1.1",
    "@lit/localize": "^0.11.2",
    "bootstrap": "^5.1.3",
    "cookie": "^0.4.2",
    "js-cookie": "^3.0.1",
    "json5": "^2.2.0",
    "lit": "^2.2.0",
    "lodash-es": "^4.17.21",
    "openid-client": "^5.1.3",
    "postgres": "mohe2015/postgres#patch-1",
    "zod": "^3.12.0"
  },
  "dependenciesComments": {
    "hot-esm": "1.2.2",
    "lit": "monorepos can't be installed from git afaik and also it probably doesnt work by default",
    "postgres": "v2.0.0-beta.11",
    "typescript": "installing the dependencies is probably more dangerous...",
    "zod": "doesn't work from git because no prepare script. Should probably be installable without any building",
    "opentelemetry": "https://nodejs.org/dist/latest-v17.x/docs/api/perf_hooks.html https://www.jaegertracing.io/"
  },
  "devDependencies": {
    "@lit/localize-tools": "^0.6.1",
    "esbuild": "^0.14.23",
    "esbuild-linux-64": "^0.14.23",
    "purgecss": "^4.1.3"
  },
  "optionalDependencies": {
    "@types/cookie": "^0.4.1",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.21",
    "@types/selenium-webdriver": "^4.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "prettier": "^2.5.1",
    "selenium-webdriver": "^4.1.1",
    "typescript": "^4.5.5"
  }
}
