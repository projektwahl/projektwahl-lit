{
  "$schema": "https://raw.githubusercontent.com/lit/lit/main/packages/localize-tools/config.schema.json",
  "sourceLocale": "en",
  "targetLocales": ["de"],
  "inputFiles": ["./src/client/**/*.js"],
  "output": {
    "outputDir": "./src/client/generated_locales/",
    "mode": "runtime"
  },
  "interchange": {
    "format": "xliff",
    "xliffDir": "./src/client/xliff/"
  }
}
